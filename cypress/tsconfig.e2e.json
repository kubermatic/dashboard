{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@ctypes/*": ["types/*"],
      "@intercept/*": ["intercept/*"]
    },
    "types": ["cypress"],
  },
  "include": [
    "integration/**/*.ts",
    "fixtures/**/*.ts",
    "pages/**/*.ts",
    "utils/*.ts",
    "support/*.ts",
    "plugins/*.ts",
    "intercept/*.ts",
    "types/*.ts",
    "../node_modules/cypress"]
}
