{
  "name": "kubermatic",
  "private": true,
  "version": "1.0.0-SNAPSHOT",
  "description": "Kubermatic",
  "repository": "https://github.com/kubermatic/dashboard-v2",
  "license": "propietary",
  "angular-cli": {},
  "scripts": {
    "start": "npm run versioninfo && ng serve --hmr",
    "start:local": "npm run versioninfo && ng serve --hmr --proxy-config proxy-local.conf.json",
    "start:mock": "concurrently --kill-others --prefix \"[{name}]\" --names \"mock-server,ng-serve\" \"node mock-server.js\" \"ng serve --hmr --proxy-config proxy-mock.conf.json\"",
    "build": "npm run versioninfo && ng build --prod",
    "test": "ng test",
    "test:headless": "ng test --browsers=ChromeHeadless --watch=false",
    "test:firefox": "ng test --browsers=FirefoxAutoAllowGUM --watch=false",
    "e2e": "ng e2e --webdriver-update false",
    "pree2e": "webdriver-manager update --standalone false --versions.chrome 2.45 --gecko false",
    "check": "npm run check:ts && npm run check:scss",
    "check:ts": "gts check",
    "check:scss": "stylelint \"src/**/*.scss\"",
    "fix": "npm run fix:ts && npm run fix:scss",
    "fix:ts": "gts fix",
    "fix:scss": "stylelint \"src/**/*.scss\" --fix",
    "clean": "gts clean",
    "versioninfo": "node git-version.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "gts fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@angular-redux/form": "^9.0.1",
    "@angular-redux/store": "^9.0.0",
    "@angular/animations": "7.2.1",
    "@angular/cdk": "7.2.1",
    "@angular/common": "7.2.1",
    "@angular/compiler": "7.2.1",
    "@angular/core": "7.2.1",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "7.2.1",
    "@angular/http": "7.2.1",
    "@angular/material": "7.2.1",
    "@angular/platform-browser": "7.2.1",
    "@angular/platform-browser-dynamic": "7.2.1",
    "@angular/router": "7.2.1",
    "angular-particle": "^1.0.4",
    "angular2-notifications": "^2.0.0",
    "animate.css": "^3.5.2",
    "core-js": "^2.6.2",
    "flag-icon-css": "^3.0.0",
    "git-describe": "^4.0.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "karma-spec-reporter": "0.0.32",
    "lodash": "^4.17.4",
    "moment": "^2.23.0",
    "ng2-validation": "4.2.0",
    "ngx-clipboard": "11.1.9",
    "ngx-cookie-service": "^2.1.0",
    "ngx-window-token": "^1.0.0",
    "openstack-lib": "0.0.2",
    "redux": "^4.0.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "6.3.3",
    "semver": "^5.5.0",
    "ts-helpers": "1.1.2",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.12.2",
    "@angular/cli": "7.2.2",
    "@angular/compiler-cli": "7.2.1",
    "@ngrx/store": "^7.0.0",
    "@types/jasmine": "3.3.6",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.0.2",
    "concurrently": "^4.0.1",
    "gts": "^0.9.0",
    "husky": "^1.3.1",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-istanbul": "0.6.0",
    "lint-staged": "^8.1.0",
    "ng2-mock-component": "^0.1.1",
    "ng2-slim-loading-bar": "4.0.0",
    "ngrx-store-freeze": "0.2.4",
    "ngrx-store-logger": "0.2.2",
    "protractor": "5.4.2",
    "request": "2.88.0",
    "stylelint": "^9.10.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "7.0.1",
    "tslint": "^5.12.1",
    "typescript": "3.1.6",
    "webdriver-manager": "12.1.1"
  }
}
