{
  "name": "kubermatic",
  "private": true,
  "version": "1.0.0-SNAPSHOT",
  "description": "Kubermatic",
  "repository": "https://github.com/kubermatic/dashboard-v2",
  "license": "proprietary",
  "angular-cli": {},
  "scripts": {
    "start": "npm run versioninfo && ng serve",
    "start:local": "npm run versioninfo && ng serve --configuration=local",
    "start:e2e": "npm run versioninfo && ng serve --configuration=e2e",
    "start:e2e-local": "npm run versioninfo && ng serve --configuration=e2e-local",
    "build": "npm run versioninfo && ng build --prod",
    "test": "jest",
    "test:ci": "jest --coverage -i",
    "test:watch": "jest --watch",
    "cy": "cypress run --record false --browser chrome",
    "e2e": "start-server-and-test start:e2e http-get://localhost:8000 cy",
    "e2e:local": "start-server-and-test start:e2e-local http-get://localhost:8000 cy",
    "check": "npm run check:ts && npm run check:scss && npm run check:licenses",
    "check:ts": "gts check",
    "check:scss": "stylelint \"src/**/*.scss\"",
    "check:licenses": "license-checker --summary --failOn=GPL",
    "fix": "npm run fix:ts && npm run fix:scss && npm run fix:html",
    "fix:ts": "gts fix",
    "fix:scss": "stylelint \"src/**/*.scss\" --fix",
    "fix:html": "js-beautify --end-with-newline --indent-size=2 --wrap-attributes=force-aligned --replace -f \"src/**/*.html\"",
    "clean": "gts clean",
    "versioninfo": "node git-version.js",
    "postinstall": "ngcc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "gts fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.html": [
      "js-beautify --end-with-newline --indent-size=2 --wrap-attributes=force-aligned --replace -f ",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "9.0.0-rc.8",
    "@angular/cdk": "9.0.0-rc.7",
    "@angular/common": "9.0.0-rc.8",
    "@angular/compiler": "9.0.0-rc.8",
    "@angular/core": "9.0.0-rc.8",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "9.0.0-rc.8",
    "@angular/http": "8.0.0-beta.10",
    "@angular/localize": "9.0.7",
    "@angular/material": "9.0.0-rc.7",
    "@angular/platform-browser": "9.0.0-rc.8",
    "@angular/platform-browser-dynamic": "9.0.0-rc.8",
    "@angular/router": "9.0.0-rc.8",
    "animate.css": "3.7.2",
    "buffer": "5.5.0",
    "core-js": "3.6.4",
    "flag-icon-css": "3.4.6",
    "git-describe": "4.0.4",
    "jquery": "3.4.1",
    "karma-spec-reporter": "0.0.32",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "ngx-clipboard": "13.0.0",
    "ngx-cookie-service": "2.4.0",
    "ngx-custom-validators": "8.0.0",
    "ngx-filter-pipe": "2.1.2",
    "ngx-window-token": "3.0.0",
    "openstack-lib": "0.0.2",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "semver": "7.1.3",
    "swagger-ui": "3.25.0",
    "ts-helpers": "1.1.2",
    "tslib": "1.11.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.1",
    "@angular/cli": "9.0.7",
    "@angular/compiler-cli": "9.0.0-rc.8",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/node": "13.9.2",
    "codelyzer": "5.2.1",
    "cypress": "3.8.1",
    "gts": "0.9.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jest-preset-angular": "8.1.2",
    "js-beautify": "1.10.3",
    "license-checker": "25.0.1",
    "lint-staged": "10.0.8",
    "ng2-mock-component": "0.2.0",
    "node-gyp": "6.1.0",
    "request": "2.88.2",
    "serialize-javascript": "3.0.0",
    "start-server-and-test": "1.10.11",
    "stylelint": "13.2.1",
    "stylelint-config-standard": "19.0.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.0",
    "tslint": "5.20.1",
    "typescript": "3.6.5"
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
