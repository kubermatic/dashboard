{
  "name": "kubermatic",
  "private": true,
  "version": "1.0.0-SNAPSHOT",
  "description": "Kubermatic",
  "repository": "https://github.com/kubermatic/dashboard-v2",
  "license": "propietary",
  "angular-cli": {},
  "scripts": {
    "ng:version": "ng v",
    "ng:init": "ng init",
    "serve": "ng serve --port 8000 --hmr",
    "serve:proxy": "ng serve --port 8000 --hmr --proxy-config proxy.conf.json",
    "serve:mock": "concurrently --kill-others --prefix \"[{name}]\" --names \"mock-server,ng-serve\" \"node mock-server.js\" \"ng serve --port 8000 --hmr --proxy-config proxy-mock.conf.json\"",
    "build": "ng build --prod",
    "lint": "ng lint --type-check",
    "test": "ng test",
    "test-headless": "ng test --browsers=ChromeHeadless --watch=false",
    "test-firefox": "ng test --browsers=FirefoxAutoAllowGUM --watch=false",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "tsfmt": "tsfmt --verify"
  },
  "dependencies": {
    "@angular-redux/form": "^9.0.1",
    "@angular-redux/store": "^9.0.0",
