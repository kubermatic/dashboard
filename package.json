{
  "name": "kubermatic",
  "private": true,
  "version": "1.0.0-SNAPSHOT",
  "description": "Kubermatic",
  "repository": "https://github.com/kubermatic/dashboard-v2",
  "license": "propietary",
  "angular-cli": {},
  "scripts": {
    "ng:version": "ng v",
    "ng:init": "ng init",
    "serve": "ng serve --port 8000 --hmr",
    "serve:proxy": "ng serve --port 8000 --hmr --proxy-config proxy.conf.json",
    "serve:mock": "concurrently --kill-others --prefix \"[{name}]\" --names \"mock-server,ng-serve\" \"node mock-server.js\" \"ng serve --port 8000 --hmr --proxy-config proxy-mock.conf.json\"",
    "prebuild": "ng test --single-run",
    "build": "ng build --prod",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/router": "3.4.3",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.2",
    "angular2-jwt": "0.1.28",
    "angular2-notifications": "0.4.49",
    "animate.css": "^3.5.2",
    "auth0-lock": "10.10.2",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "karma-spec-reporter": "^0.0.26",
    "ng2-validation": "3.1.2",
    "reselect": "^2.5.4",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "ng2-slim-loading-bar": "^2.3.0",
    "ngrx-store-freeze": "^0.1.6",
    "ngrx-store-logger": "^0.1.7",
    "protractor": "~4.0.13",
    "request": "^2.79.0",
    "ts-node": "1.2.1",
    "tslint": "^4.0.2",
    "typescript": "2.1.5",
    "webdriver-manager": "10.2.5"
  }
