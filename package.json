{
  "name": "kubermatic",
  "private": true,
  "version": "1.0.0-SNAPSHOT",
  "description": "Kubermatic",
  "repository": "https://github.com/kubermatic/dashboard-v2",
  "license": "proprietary",
  "angular-cli": {},
  "scripts": {
    "start": "npm run versioninfo && ng serve --hmr",
    "start:local": "npm run versioninfo && ng serve --hmr --configuration=local",
    "start:e2e": "npm run versioninfo && ng serve --configuration=e2e",
    "start:e2e-local": "npm run versioninfo && ng serve --configuration=e2e-local",
    "build": "npm run versioninfo && ng build --prod",
    "test": "jest",
    "test:ci": "jest --coverage -i",
    "test:watch": "jest --watch",
    "cy": "cypress run --record false --browser chrome",
    "e2e": "start-server-and-test start:e2e http-get://localhost:8000 cy",
    "e2e:local": "start-server-and-test start:e2e-local http-get://localhost:8000 cy",
    "check": "npm run check:ts && npm run check:scss && npm run check:licenses",
    "check:ts": "gts check",
    "check:scss": "stylelint \"src/**/*.scss\"",
    "check:licenses": "license-checker --summary --failOn=GPL",
    "fix": "npm run fix:ts && npm run fix:scss && npm run fix:html",
    "fix:ts": "gts fix",
    "fix:scss": "stylelint \"src/**/*.scss\" --fix",
    "fix:html": "js-beautify --end-with-newline --indent-size=2 --wrap-attributes=force-aligned --replace -f \"src/**/*.html\"",
    "clean": "gts clean",
    "versioninfo": "node git-version.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "gts fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.html": [
      "js-beautify --end-with-newline --indent-size=2 --wrap-attributes=force-aligned --replace -f ",
      "git add"
    ]
  },
  "dependencies": {
    "@angular-redux/form": "9.0.1",
    "@angular-redux/store": "9.0.0",
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.14",
    "@angular/http": "8.0.0-beta.10",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "angular2-notifications": "2.0.0",
    "animate.css": "3.7.2",
    "buffer": "5.4.3",
    "core-js": "3.6.3",
    "flag-icon-css": "3.4.5",
    "git-describe": "4.0.4",
    "hammerjs": "2.0.8",
    "jquery": "3.4.1",
    "karma-spec-reporter": "0.0.32",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "ng2-validation": "4.2.0",
    "ngx-clipboard": "12.3.0",
    "ngx-cookie-service": "2.2.0",
    "ngx-window-token": "2.0.1",
    "openstack-lib": "0.0.2",
    "redux": "4.0.1",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "semver": "6.3.0",
    "swagger-ui": "3.24.3",
    "ts-helpers": "1.1.2",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.801.2",
    "@angular/cli": "8.3.21",
    "@angular/compiler-cli": "8.2.14",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@ngrx/store": "8.6.0",
    "@types/jest": "24.0.25",
    "@types/lodash": "4.14.149",
    "@types/node": "13.1.6",
    "codelyzer": "5.2.1",
    "cypress": "3.8.1",
    "gts": "0.9.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "js-beautify": "1.10.2",
    "license-checker": "25.0.1",
    "lint-staged": "9.5.0",
    "ng2-mock-component": "0.2.0",
    "ngrx-store-freeze": "0.2.4",
    "ngrx-store-logger": "0.2.4",
    "node-gyp": "6.0.1",
    "request": "2.88.0",
    "serialize-javascript": "2.1.2",
    "start-server-and-test": "1.10.6",
    "stylelint": "12.0.0",
    "stylelint-config-standard": "19.0.0",
    "ts-node": "8.6.1",
    "tslint": "5.20.1",
    "tslib": "1.10.0",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
