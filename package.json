{
  "name": "kubermatic",
  "private": true,
  "version": "1.0.0-SNAPSHOT",
  "description": "Kubermatic",
  "repository": "https://github.com/kubermatic/dashboard-v2",
  "license": "propietary",
  "angular-cli": {},
  "scripts": {
    "start": "npm run versioninfo && ng serve --hmr",
    "start:local": "npm run versioninfo && ng serve --hmr --configuration=local",
    "build": "npm run versioninfo && ng build --prod",
    "test": "ng test",
    "test:headless": "ng test --browsers=ChromeHeadless --watch=false",
    "test:firefox": "ng test --browsers=FirefoxAutoAllowGUM --watch=false",
    "e2e": "ng e2e",
    "e2e:local": "ng e2e --configuration=e2e-local",
    "check": "npm run check:ts && npm run check:scss",
    "check:ts": "gts check",
    "check:scss": "stylelint \"src/**/*.scss\"",
    "fix": "npm run fix:ts && npm run fix:scss && npm run fix:html",
    "fix:ts": "gts fix",
    "fix:scss": "stylelint \"src/**/*.scss\" --fix",
    "fix:html": "js-beautify --end-with-newline --indent-size=2 --wrap-attributes=force-aligned --replace -f \"src/**/*.html\"",
    "clean": "gts clean",
    "versioninfo": "node git-version.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "gts fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.html": [
      "js-beautify --end-with-newline --indent-size=2 --wrap-attributes=force-aligned --replace -f ",
      "git add"
    ]
  },
  "dependencies": {
    "@angular-redux/form": "9.0.1",
    "@angular-redux/store": "9.0.0",
    "@angular/animations": "8.0.0",
    "@angular/cdk": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.0.0",
    "@angular/http": "8.0.0-beta.9",
    "@angular/material": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "angular-particle": "1.0.4",
    "angular2-notifications": "2.0.0",
    "animate.css": "3.7.1",
    "core-js": "2.6.9",
    "flag-icon-css": "3.3.0",
    "git-describe": "4.0.4",
    "hammerjs": "2.0.8",
    "jquery": "3.4.1",
    "karma-spec-reporter": "0.0.32",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "ng2-validation": "4.2.0",
    "ngx-clipboard": "12.1.0",
    "ngx-cookie-service": "2.2.0",
    "ngx-window-token": "2.0.1",
    "openstack-lib": "0.0.2",
    "redux": "4.0.1",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "semver": "6.1.1",
    "ts-helpers": "1.1.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.1",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "8.0.0",
    "@bahmutov/add-typescript-to-cypress": "2.1.0",
    "@ngrx/store": "^7.4.0",
    "@types/jasmine": "3.3.13",
    "@types/lodash": "^4.14.133",
    "@types/node": "^12.0.4",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.0",
    "cypress": "3.3.1",
    "gts": "^0.9.0",
    "husky": "2.3.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "js-beautify": "1.10.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-istanbul": "0.6.0",
    "lint-staged": "^8.1.7",
    "ng2-mock-component": "^0.1.1",
    "ng2-slim-loading-bar": "4.0.0",
    "ngrx-store-freeze": "0.2.4",
    "ngrx-store-logger": "0.2.3",
    "ngx-cypress-builder": "1.0.2",
    "node-gyp": "4.0.0",
    "request": "2.88.0",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "18.3.0",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "typescript": "3.4.5",
    "webdriver-manager": "13.0.0"
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
