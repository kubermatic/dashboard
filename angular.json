{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kubermatic": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "sourceMap": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.ee.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.png",
              { "glob": "**/*", "input": "node_modules/monaco-editor", "output": "assets/monaco-editor" }
            ],
            "allowedCommonJsDependencies": [
              "semver",
              "swagger-ui"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/css"
              ]
            },
            "styles": [
              "src/assets/css/root.scss",
              {
                "input": "src/assets/themes/light.scss",
                "bundleName": "light",
                "inject": false
              },
              {
                "input": "src/assets/themes/dark.scss",
                "bundleName": "dark",
                "inject": false
              },
              {
                "input": "src/assets/themes/custom.scss",
                "bundleName": "custom",
                "inject": false
              }
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "commonChunk": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "e2e": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "commonChunk": false,
              "optimization": false,
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.e2e.ts"
                }
              ]
            },
            "e2e-local": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "commonChunk": false,
              "optimization": false,
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.e2e.local.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kubermatic:build",
            "proxyConfig": "./proxy.conf.cjs",
            "port": 8000
          },
          "configurations": {
            "production": {
              "browserTarget": "kubermatic:build:production"
            },
            "local": {
              "proxyConfig": "./proxy-local.conf.cjs"
            },
            "e2e": {
              "browserTarget": "kubermatic:build:e2e"
            },
            "e2e-local": {
              "browserTarget": "kubermatic:build:e2e-local",
              "proxyConfig": "./proxy-local.conf.cjs"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kubermatic:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "codeCoverage": true,
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js"
            ],
            "styles": [
              "src/assets/css/root.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.png"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "kubermatic",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "kubermatic"
    },
    "@schematics/angular:directive": {
      "prefix": "kubermatic"
    },
    "@schematics/angular:guard": {},
    "@schematics/angular:interface": {},
    "@schematics/angular:module": {},
    "@schematics/angular:pipe": {},
    "@schematics/angular:service": {}
  },
  "cli": {
    "analytics": false
  }
}
