{
  "extends": "gts/tslint.json", // See: https://github.com/google/gts/blob/v0.9.0/tslint.json.
  "rules": {
    "curly": true,
    "eofline": true,
    "no-any": false,
    "no-console": true,
    "no-namespace": false,
    "switch-default": false,
    "typedef": [
      true,
      "call-signature"
    ],
    "variable-name": false
  }
}
